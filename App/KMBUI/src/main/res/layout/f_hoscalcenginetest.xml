<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  style="@style/FragmentStyle"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:orientation="vertical">
    <TextView android:layout_width="fill_parent" android:text="@string/hoscalcengtest_status" android:id="@+id/txtStatus" android:layout_height="wrap_content" style="@style/Theme.TextViewBlack"></TextView>
    <Spinner android:id="@+id/cboFile" style="@style/Theme.SpinnerLight" android:layout_height="wrap_content" android:layout_width="fill_parent"></Spinner>
	<Button style="@style/KMBButton" android:layout_height="wrap_content" android:id="@+id/btnTest" android:width="100dip" android:text="@string/btntest"></Button>
	<Button style="@style/KMBButton" android:layout_height="wrap_content" android:id="@+id/btnCancel" android:width="100dip" android:text="@string/cancellabel"></Button>
</LinearLayout>